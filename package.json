{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "run-s \"build:*\"",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "postcss styles --base styles --dir app/styles --watch",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "lint:eslint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx .",
    "lint:style": "prettier . --check",
    "lint:fix": "npx prettier . --write && npm run lint:eslint --fix"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.1",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "framer-motion": "^8.4.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
